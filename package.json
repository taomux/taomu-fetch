{
  "name": "taomu-fetch",
  "version": "0.0.6",
  "type": "module",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./types/main.d.ts",
  "private": false,
  "exports": {
    "types": "./types/main.d.ts",
    "import": "./dist/main.js",
    "require": "./dist/main.cjs"
  },
  "files": [
    "README.md",
    "types",
    "dist"
  ],
  "scripts": {
    "pu": "npm run test && npm run build && npm publish",
    "build": "node ./scripts/rm-dist.js && tsc --declaration & vite build",
    "dev": "npm run test:ui",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:dev": "vitest",
    "build:docs": "typedoc"
  },
  "dependencies": {
    "taomu-toolkit": "^0.1.6"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@vitest/ui": "^3.1.1",
    "taomu-logger": "^0.0.1",
    "typedoc": "^0.28.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.1",
    "vitest": "^3.1.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
